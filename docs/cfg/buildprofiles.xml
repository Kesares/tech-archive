<?xml version="1.0" encoding="UTF-8"?>
<buildprofiles xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <variables>
        <primary-color>iris</primary-color>
        <custom-favicons>../images/kesares_logo.svg</custom-favicons>
        <header-logo>../images/kesares_logo.svg</header-logo>
        <product-web-url>https://kesares.github.io</product-web-url>
    </variables>
    <shortcuts>
        <src>keymap.xml</src>
        <layout name="Windows" display-name="Windows"/>
        <layout name="Linux" display-name="Linux"/>
        <layout name="macOS" display-name="macOS"/>
    </shortcuts>
    <build-profile instance="ta">
        <variables>
            <noindex-content>true</noindex-content>
        </variables>
    </build-profile>
    <footer>
        <copyright>Kesares</copyright>
    </footer>

</buildprofiles>
